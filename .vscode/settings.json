{
    "CodeGPT.apiKey": "CodeGPT Plus Beta",
    "CodeGPT.Autocomplete.enabled": true,
    "asciidoc.antora.enableAntoraSupport": true,
    "files.associations": {
        ".fantomasignore": "ignore",
        "util.h": "c",
        "ykpiv.h": "c",
        "internal.h": "c",
        "locale": "c",
        "openssl-compat.h": "c"
    },
    "CodeGPT.Autocomplete.maxTokens": 824,
    "CodeGPT.Autocomplete.suggestionDelay": 1200,
    "FSharp.suggestGitignore": false,
    "cmake.configureArgs": [
        "-DVCPKG_APPLOCAL_DEPS=ON",
        "-DX_VCPKG_APPLOCAL_DEPS_INSTALL=ON",
        "-DVCPKG_TARGET_TRIPLET=arm64-osx"
    ],
    "vcpkg.general.enable": true,
    "vcpkg.target.hostTriplet": "arm64-osx",
    "vcpkg.target.defaultTriplet": "arm64-osx",
    "vcpkg.target.useStaticLib": false,
    "vcpkg.target.installDependencies": true,
    "vcpkg.target.preferSystemLibs": false,
    "vcpkg.target.useManifest": false,
    "CodeGPT.Autocomplete.provider": "Ollama - codestral:latest"
}